    <!-- sessionState mode="StateServer" cookieless="false" timeout="120"/ -->
    <customErrors mode="Off" />
    <compilation debug="true" targetFramework="4.8" />
    <httpRuntime targetFramework="4.8" requestPathInvalidCharacters="" />
