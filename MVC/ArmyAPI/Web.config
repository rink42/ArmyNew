<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="ArmyKey" value="ArmyAesKey" /> <!-- 加解密使用(注意修改，改了，當時線上的人會產生API失敗，必須 iisreset) -->
    <add key="Session_LegalMinute" value="120" />
	<add key="UpdatePwWaitSeconds" value="60" />
	<add key="GetArmyUnitLevel" value="7" />
	<add key="ApiPath" value="API" />
	<add key="CheckAD" value="0" /> <!-- 是否使用 AD 驗証(0：不使用，1:使用) -->
	<add key="AD_Domain" value="army.mil.tw" />
	<add key="CacheTime" value="24" /> <!-- Cache 保留時間(小時) -->
	<add key="IsDebug" value="1" />
	<add key="CheckIpPassA" value="D8F3AB13FA1D70FA8D3D298F1A654772,C1106483B7F5D3FB546B8A747A5B596D" />
	<add key="UnsortedWhere" value="and (left(vm.unit_code,1) between '1' and '3' or left(vm.unit_code,2) in ('91','95','97')or left(vm.unit_code,3) in ('05V','05X'))--陸軍、後備、資電軍" />
    <add key="EPPlus:ExcelPackage.LicenseContext" value="NonCommercial" />
  </appSettings>
  <connectionStrings>
    <add name="ArmyConnection" connectionString="Server=192.168.42.91;Database=Army;User Id=sa;Password=syscom;Connect Timeout=600" providerName="System.Data.SqlClient" />
    <add name="ArmyWebConnectionString" connectionString="Server=192.168.42.91;Database=ArmyWeb;User Id=sa;Password=syscom;Connect Timeout=600" providerName="System.Data.SqlClient" />
	<!-- <add name="ArmyWebConnectionString" connectionString="Server=127.0.0.1;Database=ArmyWeb;User Id=sa;Password=syscom;Connect Timeout=600" providerName="System.Data.SqlClient" />-->
  </connectionStrings>
  <!--
